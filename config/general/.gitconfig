[user]
	name = Kevin Daniel
	email = kevindkevdan@gmail.com
[init]
	defaultBranch = main 
[pull]
	rebase = true  
[core]
	excludesfile = /Users/kevindaniel/.config/git/.gitignore_global
	symlinks = true

[alias]
    gone = "!f() { git fetch --all --prune; git branch -vv | awk '/: gone]/{print $1}' | xargs git branch -D; }; f"

	a = add
	aa = add --all
	ai = add -i
	#############
	ap = apply
	as = apply --stat
	ac = apply --check
	#############
	ama = am --abort
	amr = am --resolved
	ams = am --skip
	#############
	b = branch
	ba = branch -a
	bd = branch -d
	bdd = branch -D
	br = branch -r
	bc = rev-parse --abbrev-ref HEAD
	bu = !git rev-parse --abbrev-ref --symbolic-full-name "@{u}"
	bs = !git-branch-status
	#############
	c = commit
	cm = commit -m
	#############
	cl = clone
	#############
	d = diff
	#############
	f = fetch
	fo = fetch origin
	#############
	l = log --oneline
	lg = log --oneline --graph --decorate
	#############
	m = merge
	ma = merge --abort
	mc = merge --continue
	ms = merge --skip
	#############
	ch = checkout
	chmst = checkout master
	chmain = checkout main 
	chb = checkout -b
	#############
	ph = push
	phf = push --force-with-lease
	#############
	pl = pull
	pb = pull --rebase
	#############
	plo = pull origin
	pbo = pull --rebase origin
	plom = pull origin master
	#############
	rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	rbs = rebase --skip
	#############
	uncommit = reset HEAD~1
	re = reset
	rh = reset HEAD
	reh = reset --hard
	rem = reset --mixed
	res = reset --soft
	rehh = reset --hard HEAD
	remh = reset --mixed HEAD
	resh = reset --soft HEAD
	rehom = reset --hard origin/master
	#############
	s = status
	sb = status -s -b
	#############
	sa = stash apply
	sc = stash clear
	sd = stash drop
	sl = stash list
	sp = stash pop
	ss = stash save
	sw = stash show
	#############
	t = tag
	td = tag -d

